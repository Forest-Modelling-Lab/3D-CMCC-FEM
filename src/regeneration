wue.c:			s->value[YEARLY_iWUE]       = 0.;
wue.c:			s->value[YEARLY_iWUE_SUN]   = 0.;
wue.c:			s->value[YEARLY_iWUE_SHADE] = 0.;
wue.c:	if( s->value[GPP] > 0. && s->value[STOMATAL_CONDUCTANCE] > 0. )
